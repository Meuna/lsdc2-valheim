{
    "name": "valheim-ec2",
    "engineType": "ec2",
    "engine": {
        "ami": "lsdc2/images/valheim",
        "instanceTypes": ["m6a.large", "m6i.large", "m7i-flex.large", "m7i.large", "m5.large", "m5a.large"],
        "iops": 4000,
        "throughput": 400,
        "fastboot": true
    },
    "ingress": {
        "udp": [2456, 2457]
    },
    "env": {
        "LSDC2_LOW_MEMORY_WARNING_MB": "2048",
        "LSDC2_LOW_MEMORY_SIGNAL_MB": "1024",
        "LSDC2_SCAN_STDOUT": "true",
        "LSDC2_WAKEUP_SENTINEL": "Game server connected"
    },
    "params": {
        "ADMIN_PLATFORMID": "Platform ID of the admin",
        "SERVER_PASS": "Password",
        "SERVER_PRESET": "Difficulty preset",
        "SERVER_EXTRA_FLAGS": "Extra server flags (-modifier raids none)"
    }
}